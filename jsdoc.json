{
  "source": {
    "include": [
      "./src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|src/sass|src/public)"
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-http-plugin"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "template": "node_modules/docdash",
    "tutorials": "./tutoriales",
    "readme": "./README.md"
  }
}
